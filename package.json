{
  "name": "gulp-minify-inline-json",
  "version": "1.4.0",
  "description": "Gulp plugin to minify inline JSON.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint **/*.js",
    "tdd": "mocha --watch",
    "test": "mocha"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "@haensl"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "engines": {
    "node": ">=6.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haensl/gulp-minify-inline-json.git"
  },
  "bugs": {
    "url": "https://github.com/haensl/gulp-minify-inline-json/issues"
  },
  "homepage": {
    "url": "https://github.com/haensl/gulp-minify-inline-json#readme"
  },
  "keywords": [
    "gulpplugin",
    "json",
    "minify",
    "inline",
    "html",
    "gulp",
    "plugin"
  ],
  "author": {
    "name": "HP Dietz",
    "email": "h.p.dietz@gmail.com",
    "twitter": "@h_p_d",
    "url": "https://hpdietz.com"
  },
  "license": "MIT",
  "dependencies": {
    "minify-inline-json": "^1.1.1",
    "plugin-error": "^1.0.1",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@haensl/eslint-config": "^1.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.19.0",
    "gulp": "^4.0.2",
    "husky": "^4.3.8",
    "mocha": "^8.2.1",
    "vinyl": "^2.2.1"
  }
}
